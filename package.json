{
  "name": "@privyid/design-system",
  "version": "0.27.0",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "yarn docs:dev",
    "prod": "yarn docs:build && yarn docs:serve",
    "build": "yarn docs:build",
    "docs:dev": "vitepress dev src --port 3000",
    "docs:build": "vitepress build src",
    "docs:serve": "vitepress serve src",
    "test": "vitest --reporter=dot",
    "typecheck": "vue-tsc --noEmit -p tsconfig.check.json",
    "lint:ci": "yarn lint",
    "lint": "eslint src/ --ext .js,.vue,.ts --format pretty",
    "fix": "yarn lint --fix",
    "coverage": "vitest run --coverage --reporter=dot",
    "postinstall": "husky install",
    "packages:prepare": "yarn workspaces foreach -A -v --topological-dev run prepare",
    "packages:build": "yarn workspaces foreach -A -v --topological-dev run build",
    "release": "yarn workspaces foreach -A -v --topological-dev --no-private npm publish --tolerate-republish",
    "release:prepare": "yarn workspaces foreach -A -v --topological-dev version",
    "icon:sync": "yarn workspace @privyid/persona-icon sync",
    "icon:changelog": "yarn workspace @privyid/persona-icon changelog"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@interactjs/types": "1.10.26",
    "@privyid/browserslist-config": "workspace:^",
    "@privyid/eslint-config-persona": "workspace:^",
    "@privyid/tailwind-preset": "workspace:^",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-inject": "^5.0.3",
    "@rollup/plugin-replace": "^5.0.1",
    "@rushstack/eslint-patch": "1.6.1",
    "@testing-library/jest-dom": "6.1.6",
    "@testing-library/vue": "8.0.1",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/lodash-es": "4.17.12",
    "@types/marked": "^5.0.1",
    "@types/sanitize-html": "^2",
    "@types/scroll-into-view": "^1",
    "@types/testing-library__jest-dom": "5.14.9",
    "@types/webfontloader": "1.6.38",
    "@types/zxcvbn": "^4",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-vue": "4.5.2",
    "@vitest/browser": "^0.25.3",
    "@vitest/coverage-c8": "0.24.5",
    "@vue/eslint-config-typescript": "12.0.0",
    "autoprefixer": "10.4.16",
    "browserslist-to-esbuild": "^1.2.0",
    "browserslist-useragent-regexp": "^4.0.0",
    "eslint": "8.55.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-formatter-pretty": "5.0.0",
    "eslint-plugin-align-assignments": "1.1.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint-plugin-varspacing": "1.2.2",
    "eslint-plugin-vue": "9.19.2",
    "happy-dom": "8.9.0",
    "husky": "8.0.3",
    "jest-extended": "4.0.2",
    "lint-staged": "15.2.0",
    "nanodelay": "2.0.2",
    "postcss": "8.4.33",
    "postcss-nested": "6.0.1",
    "tailwindcss": "3.3.6",
    "typescript": "5.3.3",
    "ufo": "^1.0.1",
    "vitepress": "1.0.0-rc.35",
    "vitest": "0.24.5",
    "vue-tsc": "1.8.27"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.4",
    "@fontsource/dm-sans": "5.0.18",
    "@juggle/resize-observer": "^3.4.0",
    "@privyid/persona-icon": "workspace:^",
    "@splidejs/splide": "^4.1.4",
    "@testing-library/user-event": "^14.4.3",
    "@vueuse/core": "10.7.0",
    "@vueuse/math": "10.3.0",
    "@zxing/browser": "0.1.4",
    "@zxing/library": "0.20.0",
    "chart.js": "4.4.1",
    "core-js": "3",
    "date-fns": "2.30.0",
    "defu": "6.1.3",
    "fast-equals": "^5.0.0",
    "fuse.js": "7.0.0",
    "interactjs": "1.10.26",
    "lodash-es": "4.17.21",
    "marked": "^5.0.0",
    "nanodelay": "2.0.2",
    "pdfjs-dist": "^3.10.111",
    "postcss-hexrgba": "^2.1.0",
    "postcss-lighten-darken": "^0.9.0",
    "sanitize-html": "^2.8.1",
    "scroll-into-view": "^1.16.2",
    "tabbable": "6.2.0",
    "vue-collapsed": "^1.2.5",
    "vue-content-loader": "^2.0.1",
    "vue-demi": "0.14.6",
    "vue-router": "4.2.5",
    "vue3-iframe": "^0.0.7",
    "vuedraggable": "next",
    "webfontloader": "1.6.28",
    "zxcvbn": "^4.4.2"
  },
  "lint-staged": {
    "*.(js|ts|vue)": [
      "eslint --fix"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}
