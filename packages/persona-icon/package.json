{
  "name": "@privyid/persona-icon",
  "description": "Persona icon package",
  "version": "1.1.2",
  "packageManager": "yarn@4.9.1",
  "type": "module",
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": {
        "import": "./dist/types.d.mts",
        "require": "./dist/types.d.ts"
      }
    },
    "./vue/*": "./vue/*",
    "./font/*": "./font/*",
    "./svg/*": "./svg/*"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/privy-open-source/design-system.git",
    "type": "git",
    "directory": "packages/persona-icon"
  },
  "author": "Ade Novid <adenvt@gmail.com> (https://github.com/adenvt)",
  "bugs": {
    "url": "https://github.com/privy-open-source/design-system/issues"
  },
  "homepage": "https://privy-open-source.github.io/design-system/",
  "files": [
    "dist/",
    "svg/",
    "vue/",
    "font/"
  ],
  "scripts": {
    "prepack": "yarn build",
    "build": "nuxt-module-build build",
    "lint": "node --import jiti/register scripts/lint.ts",
    "sync": "node --import jiti/register scripts/sync.ts",
    "changelog": "node --import jiti/register scripts/changelog.ts"
  },
  "devDependencies": {
    "@nuxt/module-builder": "1.0.1",
    "@tsconfig/node22": "22.0.1",
    "@types/fs-extra": "11.0.4",
    "@types/minimist": "1.2.5",
    "@types/svg2ttf": "5.0.3",
    "@types/ttf2eot": "2.0.2",
    "@types/ttf2woff": "2.0.4",
    "@types/ttf2woff2": "6.0.0",
    "dotenv": "16.5.0",
    "fast-glob": "3.3.3",
    "figma-api": "1.12.0",
    "fs-extra": "11.3.0",
    "jiti": "2.4.2",
    "minimist": "1.2.8",
    "ofetch": "1.4.1",
    "ohash": "2.0.11",
    "ora": "8.2.0",
    "p-all": "5.0.0",
    "svg-path-commander": "2.1.10",
    "svg2ttf": "6.0.3",
    "svgicons2svgfont": "15.0.1",
    "svgo": "3.3.2",
    "ttf2eot": "3.1.0",
    "ttf2woff": "3.0.0",
    "ttf2woff2": "6.0.1"
  },
  "dependencies": {
    "@nuxt/kit": "3.16.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
